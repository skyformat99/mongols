prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: mongols
Description: The mongols library,a network library for c++
Version: 1.2.4
URL: https://github.com/webcpp/mongols
Requires:
Libs: -L${libdir} -lmongols
Libs.private: -lpthread -ldl -lrt -m -lstdc++
Cflags: -I${includedir}/mongols -I${includedir}/mongols/lib
