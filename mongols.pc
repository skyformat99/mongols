prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: mongols
Description: C++ high performance networking with TCP/RESP/HTTP/WebSocket protocols
Version: 1.5.3.1
URL: https://github.com/webcpp/mongols
Requires:
Libs: -L${libdir} -lmongols -lpthread -ldl -lrt -lm -lstdc++
Cflags: -I${includedir}/mongols -I${includedir}/mongols/lib
